<UserControl x:Name="conra" x:Class="SolenoidTester.group.Tests.ManualTesting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             xmlns:local="clr-namespace:SolenoidTester.CustomControls"
             xmlns:demo="clr-namespace:SolenoidTester.group.Tests"
             xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="1440">
    <UserControl.DataContext>
        <demo:LiveDataViewModel/>
    </UserControl.DataContext>
    <UserControl.Resources>
        <Style TargetType="TextBlock">
            <Setter Property="FontFamily" Value="Segoe UI" />
        </Style>
        <SolidColorBrush x:Key="AccentBrush" Color="#FF8EC441" />
    </UserControl.Resources>


    <Grid Style="{StaticResource testPageContentRoot}">
        <!--<StackPanel MinWidth="200">
                <TextBlock Text="BASIC PAGE" Style="{StaticResource Heading2}" />
                <TextBlock Text="Content goes here" />
            </StackPanel>-->
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="45" />
                <RowDefinition Height="*" />
                <RowDefinition Height="45" />
            </Grid.RowDefinitions>
            <!--标题-->
            <StackPanel Orientation="Horizontal">
                <!--<TextBlock x:Name="TestItemNum" Text="*" Style="{StaticResource header_curStep}"/>
                <TextBlock Text="/4" Style="{StaticResource header_totalSteps}" />-->
                <TextBlock x:Name="TestItem" Text="手动测试" FontSize="20" Foreground="#FFFFFFFF" VerticalAlignment="Center" />
            </StackPanel>
            <!--主体-->
            <Grid Grid.Row="1" HorizontalAlignment="Left" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50*"/>
                    <ColumnDefinition Width="1123*"/>
                </Grid.ColumnDefinitions>
                <!--曲线图-->
                <StackPanel  HorizontalAlignment="Right" Grid.ColumnSpan="2" Grid.Column="0" Width="1140">
                    <StackPanel Background="#FF181818" Width="1140">
                        <WrapPanel Height="60" Margin="0,0,0,0">
                            <CheckBox x:Name="P_Source_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate1}" Style="{StaticResource testChartOption}" Click="P_Source_Check_Click" >
                                <TextBlock Style="{StaticResource chartLabel}" Text="油压源"/>
                            </CheckBox>
                            <CheckBox x:Name="P_TCC_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate2}" Style="{StaticResource testChartOption}" Click="P_TCC_Check_Click" >
                                <TextBlock Style="{StaticResource chartLabel}" Text="TCC"/>
                            </CheckBox>
                            <CheckBox x:Name="P_Line_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate3}" Style="{StaticResource testChartOption}" Click="P_Line_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="EPC"/>
                            </CheckBox>
                            <CheckBox x:Name="P_Shift_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate4}" Style="{StaticResource testChartOption}" Click="P_Shift_Check_Click" >
                                <TextBlock Style="{StaticResource chartLabel}" Text="换档电磁阀"/>
                            </CheckBox>
                            <CheckBox x:Name="P_C1234_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate5}" Style="{StaticResource testChartOption}" Click="P_C1234_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="C1234"/>
                            </CheckBox>
                            <CheckBox x:Name="P_CB26_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate6}" Style="{StaticResource testChartOption}" Click="P_CB26_Check_Click" >
                                <TextBlock Style="{StaticResource chartLabel}" Text="CB26"/>
                            </CheckBox>
                            <CheckBox x:Name="P_C35R_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate7}" Style="{StaticResource testChartOption}" Click="P_C35R_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="C35R"/>
                            </CheckBox>
                            <CheckBox x:Name="P_C456_Check" IsChecked="True" Template="{DynamicResource CheckBoxControlTemplate8}" Style="{StaticResource testChartOption}" Click="P_C456_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="C456"/>
                            </CheckBox>
                            <CheckBox x:Name="temperature_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate9}" Style="{StaticResource testChartOption}" Click="temperature_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="油温"/>
                            </CheckBox>
                            <CheckBox x:Name="inputSpeed_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate10}" Style="{StaticResource testChartOption}" Click="inputSpeed_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="输入转速"/>
                            </CheckBox>
                            <CheckBox x:Name="outputSpeed_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate11}" Style="{StaticResource testChartOption}" Click="outputSpeed_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="输出转速"/>
                            </CheckBox>
                            <CheckBox x:Name="TCUInputSpeed_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate12}" Style="{StaticResource testChartOption}" Click="TCUInputSpeed_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="TCU输入转速"/>
                            </CheckBox>
                            <CheckBox x:Name="TCUOnputSpeed_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate13}" Style="{StaticResource testChartOption}" Click="TCUOnputSpeed_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="TCU输出转速"/>
                            </CheckBox>
                            <CheckBox x:Name="TCUtemperature_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate14}" Style="{StaticResource testChartOption}" Click="TCUtemperature_Check_Click">
                                <TextBlock Style="{StaticResource chartLabel}" Text="TCU温度"/>
                            </CheckBox>
                            <CheckBox x:Name="GreaNum_Check" IsChecked="False" Template="{DynamicResource CheckBoxControlTemplate15}" Style="{StaticResource testChartOption}" Click="GreaNum_Check_Checked">
                                <TextBlock Style="{StaticResource chartLabel}" Text="需求档位"/>
                            </CheckBox>
                            <CheckBox IsChecked="{Binding UseAlignmentDate, Mode=Default}" Template="{DynamicResource CheckBoxControlTemplate8}" Style="{StaticResource testChartOption}">
                                <TextBlock Style="{StaticResource chartLabel}" Text="显示方式"/>
                            </CheckBox>
                        </WrapPanel>
                    </StackPanel>

                    <telerik:RadCartesianChart Height="350" Margin="0,0,0,0"  >

                        <telerik:RadCartesianChart.Behaviors>
                            <telerik:ChartPanAndZoomBehavior ZoomMode="Both" PanMode="Horizontal" DragMode="Pan"/>
                            <telerik:ChartTrackBallBehavior ShowIntersectionPoints="True"
                                             ShowTrackInfo="False"/>
                        </telerik:RadCartesianChart.Behaviors>
                        <telerik:RadCartesianChart.Resources>
                            <telerik:LinearAxis HorizontalLocation="Left" Maximum="2500" ElementBrush="White" Title="KPa" x:Key="pressureAxis" FontSize="10" MajorTickLength="5"/>
                            <telerik:LinearAxis HorizontalLocation="Left" Maximum="150" ElementBrush="White" Title="N.m" x:Key="torqueAxis" FontSize="10" Minimum="-150"/>
                            <telerik:LinearAxis HorizontalLocation="Left" Maximum="3000" ElementBrush="White" Title="RPM" x:Key="speedAxis" FontSize="10"/>
                            <!--<telerik:LinearAxis HorizontalLocation="Right" ElementBrush="White" Title="档位" x:Key="gearAxis" FontSize="10" Minimum="0" Maximum="10"/>-->
                        </telerik:RadCartesianChart.Resources>
                        <telerik:RadCartesianChart.EmptyContentTemplate>
                            <DataTemplate>
                                <telerik:RadBusyIndicator IsBusy="True" IsIndeterminate="True" BusyContent="读取数据中 ..." />
                            </DataTemplate>
                        </telerik:RadCartesianChart.EmptyContentTemplate>

                        <telerik:RadCartesianChart.Grid>
                            <telerik:CartesianChartGrid MajorLinesVisibility="Y" MajorYLineStyle="{StaticResource StripLinesStyle}" Opacity="0.1" />
                        </telerik:RadCartesianChart.Grid>

                        <telerik:RadCartesianChart.HorizontalAxis>
                            <telerik:DateTimeContinuousAxis MajorStepUnit="Second"
                                                LabelInterval="20"
                                                LabelFormat="mm:ss.f" FontFamily="Segoe UI"
                                                PlotMode="OnTicks"
                                                TickOrigin="{Binding AlignmentDate}" MaximumTicks="101" MajorTickLength="5" LineThickness="1"/>
                        </telerik:RadCartesianChart.HorizontalAxis>




                        <telerik:RadCartesianChart.VerticalAxis>
                            <telerik:LinearAxis Maximum="12" HorizontalLocation="Right" Visibility="Collapsed" />
                        </telerik:RadCartesianChart.VerticalAxis>



                        <!--主油压-->
                        <telerik:LineSeries x:Name="P_Source_line" Visibility="Visible" Stroke="#FF0063b1" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_Source_Data}"/>


                        <!--油压1-->
                        <telerik:LineSeries x:Name="P_TCC_line" Visibility="Visible"  Stroke="#FFc2700c" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_TCC_Data}"/>


                        <!--油压2-->
                        <telerik:LineSeries x:Name="P_Line_line" Visibility="Visible"  Stroke="#FF107c10" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_Line_Data}"/>


                        <!--油压3-->
                        <telerik:LineSeries x:Name="P_Shift_line" Visibility="Visible"  Stroke="#FFe300bc" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_Shift_Data}"/>


                        <!--油压4-->
                        <telerik:LineSeries x:Name="P_C1234_line" Visibility="Visible"   Stroke="#FF486860" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_C1234_Data}"/>


                        <!--油压5-->
                        <telerik:LineSeries x:Name="P_CB26_line" Visibility="Visible"  Stroke="#FF00b7c3" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_CB26_Data}"/>


                        <!--输入扭矩-->
                        <telerik:LineSeries x:Name="P_C35R_line" Visibility="Visible"  Stroke="#FF881798" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_C35R_Data}"/>


                        <!--左输出扭矩-->
                        <telerik:LineSeries x:Name="P_C456_line" Visibility="Visible"  Stroke="#FF00cc6a" StrokeThickness="2" VerticalAxis="{StaticResource pressureAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding P_C456_Data}"/>


                        <!--右输出扭矩-->
                        <telerik:LineSeries x:Name="temperature_line" Visibility="Collapsed" Stroke="#FFff4343" StrokeThickness="2" VerticalAxis="{StaticResource torqueAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding temperature_Data}"/>


                        <!--输入转速-->
                        <telerik:LineSeries x:Name="inputSpeed_line" Visibility="Collapsed" Stroke="#FFc30052" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding inputSpeed_Data}"/>

                        <!--左输出转速-->
                        <telerik:LineSeries x:Name="outputSpeed_line" Visibility="Collapsed" Stroke="#FF9a0089" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding outputSpeed_Data}"/>


                        <!--右输出转速-->
                        <telerik:LineSeries x:Name="TCUInputSpeed_line" Visibility="Collapsed" Stroke="#FF5d5a58" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding TCUInputSpeed_Data}"/>


                        <!--输出转速(CAN)-->
                        <telerik:LineSeries x:Name="TCUOnputSpeed_line" Visibility="Collapsed" Stroke="#FF847545" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding TCUOnputSpeed_Data}"/>


                        <!--输入转速(CAN)-->
                        <telerik:LineSeries x:Name="TCUtemperature_line" Visibility="Collapsed" Stroke="#FF68768a" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding TCUtemperature_Data}"/>


                        <!--需求档位-->
                        <telerik:LineSeries x:Name="GreaNum_line" Visibility="Collapsed" Stroke="#FF038387" StrokeThickness="2" VerticalAxis="{StaticResource speedAxis}" ValueBinding="Value" CategoryBinding="Category" ItemsSource="{Binding GreaNum_Data}"/>

                    </telerik:RadCartesianChart>


                    <StackPanel  Height="30"  HorizontalAlignment="Left" Orientation="Horizontal" Background="#4C000000" Width="1140" >
                        <Label Width="20"/>
                        <Button x:Name="btn_TCC_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_TCC_Press_Click" FontWeight="Bold"/>
                        <Label Width="38"/>
                        <Button x:Name="btn_EPC_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_EPC_Press_Click" FontWeight="Bold"/>
                        <Label Width="43"/>
                        <Button x:Name="btn_Shift_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_Shift_Press_Click" FontWeight="Bold"/>
                        <Label Width="50"/>
                        <Button x:Name="btn_C1234_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_C1234_Press_Click" FontWeight="Bold"/>
                        <Label Width="35"/>
                        <Button x:Name="btn_CB26_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_CB26_Press_Click" FontWeight="Bold"/>
                        <Label Width="38"/>
                        <Button x:Name="btn_C35R_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_C35R_Press_Click" FontWeight="Bold"/>
                        <Label Width="38"/>
                        <Button x:Name="btn_C456_Press" Width="120" Content="全       压" Background="#FF60A917" FontSize="16" Click="btn_C456_Press_Click" FontWeight="Bold"/>
                    </StackPanel>
                    <StackPanel  Height="160"  HorizontalAlignment="Left" Orientation="Horizontal" Background="#4C000000" Width="1140" >

                        <StackPanel Height="155" Width="120" Margin="30,0,0,0" >
                            <Grid Height="155" Width="120">
                                <StackPanel Style="{StaticResource monitorHeader}">
                                    <TextBlock Text="//" Style="{StaticResource monitorHeaderLabel}" />
                                    <TextBlock Text="TCC电磁阀油压" Style="{StaticResource monitorTitle}" />
                                </StackPanel>
                                <Grid Style="{StaticResource barGaugeContainer}">
                                    <telerik:RadVerticalLinearGauge Width="120" Height="Auto" OuterBorderThickness="0" OuterBackground="Transparent" Background="Transparent" BorderBrush="Transparent" OuterBorderBrush="Transparent" Foreground="White" BorderThickness="0" Margin="0,15,0,0">
                                        <telerik:VerticalLinearScale Name="targetPressure1" Min="0" Max="1000" MajorTickStep="200" RelativeWidth="1" RelativeHeight="0.8" RelativeY="0" RelativeX="0.5" StartWidth="0.01" EndWidth="0.01" MajorTickBackground="#19FFFFFF" MiddleTickBackground="Transparent" MinorTickBackground="Transparent" LabelOffset="32" StrokeThickness="0" FontSize="10" FontFamily="Arial Narrow" MajorTickLocation="Outside" MajorTickOffset="-53"  MinorTicks="5"  MajorTickRelativeWidth="80" Fill="Transparent" Foreground="#99FFFFFF">
                                            <telerik:VerticalLinearScale.Indicators>
                                                <!--柱状图对应值-->
                                                <telerik:BarIndicator x:Name="Tcccon1_1" Value="200" BorderBrush="Transparent" Background="Red" Margin="0,0,0,0" StartWidth="0.3" FontSize="14"/>
                                            </telerik:VerticalLinearScale.Indicators>
                                            <telerik:VerticalLinearScale.CustomItems>
                                                <Border BorderBrush="#72FFFFFF" BorderThickness="1" telerik:ScaleObject.Value="0" telerik:ScaleObject.Location="OverCenter" telerik:ScaleObject.RelativeHeight="0.8*" telerik:ScaleObject.RelativeWidth="1*" Width="79" Height="1" Margin="33,0,0,0" HorizontalAlignment="Left" />
                                            </telerik:VerticalLinearScale.CustomItems>
                                        </telerik:VerticalLinearScale>
                                    </telerik:RadVerticalLinearGauge>
                                    <!--柱状图对应值-->
                                    <TextBlock x:Name="Tcccon1_2" Style="{StaticResource barGaugeValue}" Text="200"/>
                                    <TextBlock Style="{StaticResource barGaugeUnit}" Text="KPa" />
                                </Grid>
                            </Grid>



                        </StackPanel>
                        <StackPanel Height="155" Width="120" Margin="40,0,0,0" >
                            <Grid Height="155" Width="120">
                                <StackPanel Style="{StaticResource monitorHeader}">
                                    <TextBlock Text="//" Style="{StaticResource monitorHeaderLabel}" />
                                    <TextBlock Text="EPC电磁阀油压" Style="{StaticResource monitorTitle}" />
                                </StackPanel>
                                <Grid Style="{StaticResource barGaugeContainer}">
                                    <telerik:RadVerticalLinearGauge Width="120" Height="Auto" OuterBorderThickness="0" OuterBackground="Transparent" Background="Transparent" BorderBrush="Transparent" OuterBorderBrush="Transparent" Foreground="White" BorderThickness="0" Margin="0,15,0,0">
                                        <telerik:VerticalLinearScale Name="targetPressure2" Min="0" Max="1000" MajorTickStep="200" RelativeWidth="1" RelativeHeight="0.8" RelativeY="0" RelativeX="0.5" StartWidth="0.01" EndWidth="0.01" MajorTickBackground="#19FFFFFF" MiddleTickBackground="Transparent" MinorTickBackground="Transparent" LabelOffset="32" StrokeThickness="0" FontSize="10" FontFamily="Arial Narrow" MajorTickLocation="Outside" MajorTickOffset="-53"  MinorTicks="5"  MajorTickRelativeWidth="80" Fill="Transparent" Foreground="#99FFFFFF">
                                            <telerik:VerticalLinearScale.Indicators>
                                                <!--柱状图对应值-->
                                                <telerik:BarIndicator x:Name="Tcccon2_1" Value="200" BorderBrush="Transparent" Background="Orange" Margin="0,0,0,0" StartWidth="0.3" FontSize="14"/>
                                            </telerik:VerticalLinearScale.Indicators>
                                            <telerik:VerticalLinearScale.CustomItems>
                                                <Border BorderBrush="#72FFFFFF" BorderThickness="1" telerik:ScaleObject.Value="0" telerik:ScaleObject.Location="OverCenter" telerik:ScaleObject.RelativeHeight="0.8*" telerik:ScaleObject.RelativeWidth="1*" Width="79" Height="1" Margin="33,0,0,0" HorizontalAlignment="Left" />
                                            </telerik:VerticalLinearScale.CustomItems>
                                        </telerik:VerticalLinearScale>
                                    </telerik:RadVerticalLinearGauge>
                                    <!--柱状图对应值-->
                                    <TextBlock x:Name="Tcccon2_2" Style="{StaticResource barGaugeValue}" Text="200"/>
                                    <TextBlock Style="{StaticResource barGaugeUnit}" Text="KPa" />
                                </Grid>
                            </Grid>

                        </StackPanel>
                        <StackPanel Height="155" Width="120" Margin="40,0,0,0">
                            <Grid Height="155" Width="120">
                                <StackPanel Style="{StaticResource monitorHeader}">
                                    <TextBlock Text="//" Style="{StaticResource monitorHeaderLabel}" />
                                    <TextBlock Text="换档电磁阀油压" Style="{StaticResource monitorTitle}" />
                                </StackPanel>
                                <Grid Style="{StaticResource barGaugeContainer}">
                                    <telerik:RadVerticalLinearGauge Width="120" Height="Auto" OuterBorderThickness="0" OuterBackground="Transparent" Background="Transparent" BorderBrush="Transparent" OuterBorderBrush="Transparent" Foreground="White" BorderThickness="0" Margin="0,15,0,0">
                                        <telerik:VerticalLinearScale Name="targetPressure3" Min="0" Max="1000" MajorTickStep="200" RelativeWidth="1" RelativeHeight="0.8" RelativeY="0" RelativeX="0.5" StartWidth="0.01" EndWidth="0.01" MajorTickBackground="#19FFFFFF" MiddleTickBackground="Transparent" MinorTickBackground="Transparent" LabelOffset="32" StrokeThickness="0" FontSize="10" FontFamily="Arial Narrow" MajorTickLocation="Outside" MajorTickOffset="-53"  MinorTicks="5"  MajorTickRelativeWidth="80" Fill="Transparent" Foreground="#99FFFFFF">
                                            <telerik:VerticalLinearScale.Indicators>
                                                <!--柱状图对应值-->
                                                <telerik:BarIndicator x:Name="Tcccon3_1" Value="200" BorderBrush="Transparent" Background="Yellow" Margin="0,0,0,0" StartWidth="0.3" FontSize="14"/>
                                            </telerik:VerticalLinearScale.Indicators>
                                            <telerik:VerticalLinearScale.CustomItems>
                                                <Border BorderBrush="#72FFFFFF" BorderThickness="1" telerik:ScaleObject.Value="0" telerik:ScaleObject.Location="OverCenter" telerik:ScaleObject.RelativeHeight="0.8*" telerik:ScaleObject.RelativeWidth="1*" Width="79" Height="1" Margin="33,0,0,0" HorizontalAlignment="Left" />
                                            </telerik:VerticalLinearScale.CustomItems>
                                        </telerik:VerticalLinearScale>
                                    </telerik:RadVerticalLinearGauge>
                                    <!--柱状图对应值-->
                                    <TextBlock x:Name="Tcccon3_2" Style="{StaticResource barGaugeValue}" Text="200"/>
                                    <TextBlock Style="{StaticResource barGaugeUnit}" Text="KPa" />
                                </Grid>
                            </Grid>

                        </StackPanel>
                        <StackPanel Height="155" Width="120" Margin="40,0,0,0">
                            <Grid Height="155" Width="120">
                                <StackPanel Style="{StaticResource monitorHeader}">
                                    <TextBlock Text="//" Style="{StaticResource monitorHeaderLabel}" />
                                    <TextBlock Text="C1234电磁阀油压" Style="{StaticResource monitorTitle}" />
                                </StackPanel>
                                <Grid Style="{StaticResource barGaugeContainer}">
                                    <telerik:RadVerticalLinearGauge Width="120" Height="Auto" OuterBorderThickness="0" OuterBackground="Transparent" Background="Transparent" BorderBrush="Transparent" OuterBorderBrush="Transparent" Foreground="White" BorderThickness="0" Margin="0,15,0,0">
                                        <telerik:VerticalLinearScale Name="targetPressure4" Min="0" Max="1000" MajorTickStep="200" RelativeWidth="1" RelativeHeight="0.8" RelativeY="0" RelativeX="0.5" StartWidth="0.01" EndWidth="0.01" MajorTickBackground="#19FFFFFF" MiddleTickBackground="Transparent" MinorTickBackground="Transparent" LabelOffset="32" StrokeThickness="0" FontSize="10" FontFamily="Arial Narrow" MajorTickLocation="Outside" MajorTickOffset="-53"  MinorTicks="5"  MajorTickRelativeWidth="80" Fill="Transparent" Foreground="#99FFFFFF">
                                            <telerik:VerticalLinearScale.Indicators>
                                                <!--柱状图对应值-->
                                                <telerik:BarIndicator x:Name="Tcccon4_1" Value="200" BorderBrush="Transparent" Background="Lime" Margin="0,0,0,0" StartWidth="0.3" FontSize="14"/>
                                            </telerik:VerticalLinearScale.Indicators>
                                            <telerik:VerticalLinearScale.CustomItems>
                                                <Border BorderBrush="#72FFFFFF" BorderThickness="1" telerik:ScaleObject.Value="0" telerik:ScaleObject.Location="OverCenter" telerik:ScaleObject.RelativeHeight="0.8*" telerik:ScaleObject.RelativeWidth="1*" Width="79" Height="1" Margin="33,0,0,0" HorizontalAlignment="Left" />
                                            </telerik:VerticalLinearScale.CustomItems>
                                        </telerik:VerticalLinearScale>
                                    </telerik:RadVerticalLinearGauge>
                                    <!--柱状图对应值-->
                                    <TextBlock x:Name="Tcccon4_2" Style="{StaticResource barGaugeValue}" Text="200"/>
                                    <TextBlock Style="{StaticResource barGaugeUnit}" Text="KPa" />
                                </Grid>
                            </Grid>

                        </StackPanel>
                        <StackPanel Height="155" Width="120" Margin="40,0,0,0" >
                            <Grid Height="155" Width="120">
                                <StackPanel Style="{StaticResource monitorHeader}">
                                    <TextBlock Text="//" Style="{StaticResource monitorHeaderLabel}" />
                                    <TextBlock Text="CB26电磁阀油压" Style="{StaticResource monitorTitle}" />
                                </StackPanel>
                                <Grid Style="{StaticResource barGaugeContainer}">
                                    <telerik:RadVerticalLinearGauge Width="120" Height="Auto" OuterBorderThickness="0" OuterBackground="Transparent" Background="Transparent" BorderBrush="Transparent" OuterBorderBrush="Transparent" Foreground="White" BorderThickness="0" Margin="0,15,0,0">
                                        <telerik:VerticalLinearScale Name="targetPressure5" Min="0" Max="1000" MajorTickStep="200" RelativeWidth="1" RelativeHeight="0.8" RelativeY="0" RelativeX="0.5" StartWidth="0.01" EndWidth="0.01" MajorTickBackground="#19FFFFFF" MiddleTickBackground="Transparent" MinorTickBackground="Transparent" LabelOffset="32" StrokeThickness="0" FontSize="10" FontFamily="Arial Narrow" MajorTickLocation="Outside" MajorTickOffset="-53"  MinorTicks="5"  MajorTickRelativeWidth="80" Fill="Transparent" Foreground="#99FFFFFF">
                                            <telerik:VerticalLinearScale.Indicators>
                                                <!--柱状图对应值-->
                                                <telerik:BarIndicator x:Name="Tcccon5_1" Value="200" BorderBrush="Transparent" Background="#FF007FFF" Margin="0,0,0,0" StartWidth="0.3" FontSize="14"/>
                                            </telerik:VerticalLinearScale.Indicators>
                                            <telerik:VerticalLinearScale.CustomItems>
                                                <Border BorderBrush="#72FFFFFF" BorderThickness="1" telerik:ScaleObject.Value="0" telerik:ScaleObject.Location="OverCenter" telerik:ScaleObject.RelativeHeight="0.8*" telerik:ScaleObject.RelativeWidth="1*" Width="79" Height="1" Margin="33,0,0,0" HorizontalAlignment="Left" />
                                            </telerik:VerticalLinearScale.CustomItems>
                                        </telerik:VerticalLinearScale>
                                    </telerik:RadVerticalLinearGauge>
                                    <!--柱状图对应值-->
                                    <TextBlock x:Name="Tcccon5_2" Style="{StaticResource barGaugeValue}" Text="200"/>
                                    <TextBlock Style="{StaticResource barGaugeUnit}" ><Run Text="KPa"/><InlineUIContainer>
                                            
                                        </InlineUIContainer></TextBlock>
                                </Grid>
                            </Grid>

                        </StackPanel>
                        <StackPanel Height="155" Width="120" Margin="40,0,0,0" >
                            <Grid Height="155" Width="120">
